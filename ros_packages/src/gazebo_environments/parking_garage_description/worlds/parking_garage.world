  <!--   parking garage   -->
  <include>
      <pose>-30 0 0.2 0 0 1.57</pose> 
      <uri>model://parking_garage_description/models/parking_garage_collapsed</uri>
   </include>

  <include>
      <pose>-21 0 6 0 0 1.57</pose> 
