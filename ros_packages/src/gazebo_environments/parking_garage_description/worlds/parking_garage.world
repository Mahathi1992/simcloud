   </include>

   <!--   Humans   -->

<model name="human">
    <static>true</static>
   <pose>-35 9 17.45 0 3.14 0</pose>
    <link name="body"> 
   <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://human_resting_description/gazebo_model/meshes/man_resting.dae</uri>
          </mesh>
        </geometry>
